<% @page_title = "New Section" %>
<%= link_to("<< Back to List", {:action => 'list'}, :class => 'back-link') %>

<div class="page new">
  <h2>Create page</h2>

  <%=form_for(:section, :url => {:action => 'create'}) do |f| %>
  	<%= render(:partial => 'form', :locals => {:f => f})  %>
  	
  	   	<div class="form-buttons">
      		<%= submit_tag("Create page") %>
    	</div>
  <% end %>
 </div>
